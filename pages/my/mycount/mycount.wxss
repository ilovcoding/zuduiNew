image {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  border-radius: 50%;
  margin: 10rpx;  
  margin-left: 40rpx;
}

.list {
  width: 100%;
  height:70rpx;
  border-bottom: 2rpx rgb(245,245,245) solid;
  position: relative;
  font-weight:bolder;
  font-size: 24rpx;
  color: #c2c2c2;
}

.list text{
  position: absolute;
  top:20rpx;
}

.head {
  background: #f5f5f5;
  width: 100%;
  height: 70rpx;
}

.head text {
  display: inline-block;
  color: #666;
  margin: 20rpx;
  font-size: 28rpx;
}
